{
  "name": "careset/zermelo_installer",
  "description": "The installer package for Zermelo",
  "type": "library",
  "license": "Apache-2.0",
  "keywords": [
    "laravel",
    "reporting",
    "reports",
    "report",
    "datatables",
    "d3",
    "vega"
  ],
  "require": {
    "php": ">=7.2.0",
    "careset/zermelo": "^1.0",
    "careset/zermelobladetabular": "^1.0",
    "careset/zermelobladecard": "^1.0",
    "careset/zermelobladegraph": "^1.0",
    "doctrine/sql-formatter": "^1.1"
  },
  "autoload": {
    "psr-4" : {
      "CareSet\\ZermeloInstaller\\" : "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "CareSet\\ZermeloInstaller\\ServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  }
}
