{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:CareSet/CareSetJWTAuthClient.git"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "bestmomo/nice-artisan": "^1.1",
        "careset/caresetjwtauthclient": "dev-master",
        "fideloper/proxy": "~3.3",
        "laravel/framework": "5.5.*",
        "laravel/socialite": "^3.0",
        "laravel/tinker": "~1.0",
        "laravelcollective/html": "^5.4.0",
        "nesbot/carbon": "^1.22",
        "phpunit/phpunit": "^7.3",
        "proai/laravel-handlebars": "^1.7",
	"smartystreets/phpsdk": "^4.5.7",
        "spatie/laravel-tail": "^2.0",
        "studio/laravel-totem": "^2.2",
        "sunra/php-simple-html-dom-parser": "^1.5",
        "superbalist/flysystem-google-storage": "^7.0",
        "tymon/jwt-auth": "^0.5.12",
        "vlucas/phpdotenv": "^2.4",
	"wrapi/slack": "^1.0"
    },
    "require-dev": {
        "careset/durc": "dev-master",
        "careset/zermelo": "dev-master",
        "careset/zermelobladecard": "dev-master",
        "careset/zermelobladetabular": "dev-master",
        "filp/whoops": "~2.0",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "~1.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "github-oauth": {
            "github.com": "0323848d504c07a48c71d299046a79f9ffdb7fc0"
        },
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
